Include /Users/hknecht/.colima/ssh_config

Host *.compute.amazonaws.com
  AddKeysToAgent yes
  UseKeychain yes
  IdentityFile ~/.ssh/hans-aws
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null
  User root
  LogLevel QUIET
  
Host *
  AddKeysToAgent yes
  UseKeychain yes
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null
  LogLevel QUIET
